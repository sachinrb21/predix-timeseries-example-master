<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" configfailurepolicy="continue" name="Test" parallel="methods" data-provider-thread-count="20">
  <test name="Tests for Test">
    <groups>
      <run>
        <include name="Test_Script"/>
      </run>
    </groups>
    <packages>
      <package name="com.tests.*"/>
    </packages>
    <classes>
      <class name="com.tests.SampleTest"/>
    </classes>
  </test> <!-- Tests for Test -->
</suite> <!-- Test -->
